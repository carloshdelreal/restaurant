{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "import/no-unresolved": [2,{"commonjs": true, "amd": true}
    ]
  }
}
